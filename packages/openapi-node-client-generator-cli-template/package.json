{
  "name": "@forsakringskassan/openapi-node-client-generator-cli-template",
  "version": "2.5.1",
  "description": "Template files",
  "keywords": [
    "openapi"
  ],
  "homepage": "https://github.com/Forsakringskassan/openapi-node-client-generator-cli",
  "bugs": "https://github.com/Forsakringskassan/openapi-node-client-generator-cli/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forsakringskassan/openapi-node-client-generator-cli.git",
    "directory": "packages/openapi-node-client-generator-cli-template"
  },
  "license": "UNLICENSED",
  "author": "FK IT",
  "main": "./dist/typescript-fetch/index.cjs",
  "files": [
    "dist",
    "!**/*.sh",
    "!**/.openapi-generator",
    "!**/.openapi-generator-ignore"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/generators/fetch/index.d.ts",
      "import": "./dist/generators/fetch/index.mjs",
      "require": "./dist/generators/fetch/index.cjs"
    },
    "./fetch": {
      "types": "./dist/generators/fetch/index.d.ts",
      "import": "./dist/generators/fetch/index.mjs",
      "require": "./dist/generators/fetch/index.cjs"
    },
    "./mock": {
      "types": "./dist/mock/index.d.ts",
      "import": "./dist/mock/index.mjs",
      "require": "./dist/mock/index.cjs"
    },
    "./browser-mock": {
      "import": "./dist/browser-mock.mjs"
    },
    "./dist/typescript-fetch": "./dist/typescript-fetch/index.js"
  },
  "scripts": {
    "prebuild": "npm run prettier:write",
    "build": "run-s generate:* && cp openapi.yaml dist",
    "postbuild": "tsc",
    "generate:javascript-closure-angular": "openapi-node-generator-cli-npm generate -g javascript-closure-angular -i openapi.yaml -o dist/javascript-closure-angular --additional-properties useEs6=true",
    "generate:typescript-fetch": "openapi-node-generator-cli-npm generate -g typescript-fetch -t template/typescript-fetch -i openapi.yaml -o typescript-fetch --additional-properties supportsES6=true,typescriptThreePlus=true",
    "generate:openapi-complete-spec": "openapi-node-generator-cli-npm generate -g openapi-yaml -i openapi.yaml -o dist/openapi-complete-spec",
    "generate:copy-id": "[ -d ci ] && cp -rvu ci dist || true",
    "generate:bundle": "node build.mjs",
    "generate:typings": "dts-bundle-generator --config dts-config.js",
    "prettier:write": "prettier --no-error-on-unmatched-pattern \"src/mock/{**/,}*.{js,json,ts}\" --write",
    "git:clean": "git clean -fdx"
  },
  "devDependencies": {
    "@forsakringskassan/apimock-express": "2.12.0",
    "@forsakringskassan/prettier-config": "3.3.2",
    "@types/node": "24.10.7",
    "dts-bundle-generator": "9.5.1",
    "esbuild": "0.27.2",
    "esbuild-plugin-esm-cjs-shim": "0.1.0",
    "glob": "11.1.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "uuid": "13.0.0"
  },
  "dependencies": {
    "@forsakringskassan/openapi-node-generator-cli-npm": "2.5.0"
  },
  "peerDependencies": {
    "@forsakringskassan/apimock-express": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@forsakringskassan/apimock-express": {
      "optional": true
    },
    "axios": {
      "optional": true
    }
  },
  "prettier": "@forsakringskassan/prettier-config",
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 9.0.0"
  }
}
